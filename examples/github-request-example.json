{
  "id": "github-request-example",
  "name": "GitHub Request Example",
  "nodes": [
    {
      "id": "debug-nil",
      "function": "fuse/pkg/debug/nil"
    },
    {
      "id": "github-http-request",
      "function": "fuse/pkg/http/request"
    },
    {
      "id": "if-request-failed",
      "function": "fuse/pkg/logic/if"
    },
    {
      "id": "debug-print",
      "function": "fuse/pkg/debug/print"
    }
  ],
  "edges": [
    {
      "id": "0196cf0f-8a70-7463-a594-3e2a734be14c",
      "from": "debug-nil",
      "to": "github-http-request",
      "input": [
        {
          "source": "schema",
          "value": "https://api.github.com",
          "mapTo": "host"
        },
        {
          "source": "schema",
          "value": "/users/octocat",
          "mapTo": "path"
        },
        {
          "source": "schema",
          "value": "GET",
          "mapTo": "method"
        }
      ]
    },
    {
      "id": "01982dc2-1e89-7f09-be6e-26d6ed1eac4f",
      "from": "github-http-request",
      "to": "if-request-failed",
      "input": [
        {
          "source": "flow",
          "variable": "github-http-request.status",
          "mapTo": "statusCode"
        },
        {
          "source": "schema",
          "value": "statusCode <= 299",
          "mapTo": "expression"
        }
      ]
    },
    {
      "id": "01982dc1-f1f4-7371-a2b2-0a6e5a5b3a1a",
      "from": "if-request-failed",
      "to": "debug-print",
      "conditional": {
        "name": "if-true",
        "value": true
      },
      "input": [
        {
          "source": "flow",
          "variable": "github-http-request.body.login",
          "mapTo": "loginName"
        },
        {
          "source": "flow",
          "variable": "github-http-request.status",
          "mapTo": "statusCode"
        },
        {
          "source": "schema",
          "value": "Request for {{loginName}} succeeded with status code: {{statusCode}}",
          "mapTo": "message"
        }
      ]
    },
    {
      "id": "01982dc1-c106-767a-a1bc-e36611ac4488",
      "from": "if-request-failed",
      "to": "debug-print",
      "conditional": {
        "name": "if-false",
        "value": false
      },
      "input": [
        {
          "source": "flow",
          "variable": "github-http-request.status",
          "mapTo": "statusCode"
        },
        {
          "source": "schema",
          "value": "Request failed with status code: {{statusCode}}",
          "mapTo": "message"
        }
      ]
    }
  ]
}
